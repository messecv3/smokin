{
  "redirects": [
    {
      "source": "/discord",
      "destination": "https://discord.gg/rider",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "header",
          "key": "x-forwarded-proto",
          "value": "http"
        }
      ],
      "destination": "https://smokin.cc/:path*",
      "permanent": true
    }
  ]
}
